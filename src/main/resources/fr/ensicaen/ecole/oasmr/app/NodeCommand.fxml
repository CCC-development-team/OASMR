<VBox spacing="20" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1">
   <stylesheets>
      <URL value="@css/style.css" />
   </stylesheets>
   <HBox alignment="CENTER" spacing="20">
      <JFXTextField fx:id="searchField" prefHeight="26.0" prefWidth="396.0" promptText="Search" />
      <JFXButton fx:id="searchBtn">Search</JFXButton>
      <VBox.margin>
         <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
      </VBox.margin>
   </HBox>
   <FlowPane fx:id="commandFlowPane" hgap="10.0" vgap="10.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1">
      <padding>
         <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
      </padding>
      <VBox.margin>
         <Insets bottom="10.0" left="10.0" right="10.0" />
      </VBox.margin>
   </FlowPane>
</VBox>
